@import "./utils/iconfont.wxss";
@import "./wxParse/wxParse.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 100rpx 0;
  line-height: 50rpx;
  box-sizing: border-box;
} 
.footer {
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 99999999;
  width: 100%;
  display: flex;
  flex-direction: row;
  background-color: #fff;
  border-top: 1px solid #D2D2D2;
}
.noData{
  text-align: center;
  color: #969696;
  margin-top: 200rpx;
  font-size: 24rpx;
}
.borderNone{
  border: none!important;
}
.color33{
  color: #333;
}
.fl{float: left;}
.fr{float: right;}
.clear{zoom: 1;}
.clear:after{content: " ";display: block;clear: both;}