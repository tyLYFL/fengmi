/*<!--弹出海报-->*/
.propPosterBox{
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0 ,0, 0.8);
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%;
  align-content: center;
  align-items: center;
  z-index: 999;
}
.propPosterPre{
  position: relative;
  width:70%;
  height: 100%;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
}
.propPosterBox canvas{
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  border-radius: 10rpx;
}
.posterBtn{
  position: absolute;
  bottom:-5%;
  right:1rpx;
  width:100%;
  display:-webkit-flex;
  justify-content:space-between;

}
.posterBtn image{
  width: 48rpx;
  height: 48rpx;
}

/* 学员按钮 */
.bottom-item{
  width: 100%;
  height: 120rpx;
  display: -webkit-flex;
  position: fixed;
  bottom:0px;
  left:0px;
  justify-content: space-around;
  align-items: center;
  font-size: 30rpx;
  background: #fff;
}
.bottom-item-child{
   width: 320rpx;
   height: 80rpx;
   border:1px solid #FFDB60;
   color: #FFDB60;
   display: -webkit-flex;
   align-items: center;
   border-radius: 10rpx;
   justify-content: center
}
.bottom-item-child:nth-last-child(1){
