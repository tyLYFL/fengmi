.swiper-tab{ 
  display: -webkit-flex;
  display: flex;
  justify-content: space-between;
  border-bottom: 1rpx solid #e7e7e7;
  line-height: 80rpx;
  margin-top:184rpx;
  position:fixed;
  top:-7rpx;
  left:0rpx;
  width:100%;
}  
.swiper-tab.active{
  margin-top:360rpx;
}
.swiper-tab-list{
    font-size: 30rpx;
    display: inline-block;
    flex: 1;
    color: #333;
    text-align: center;
    padding: 10rpx 0;
    position: relative;
    vertical-align: -webkit-baseline-middle;
} 
.swiper-tab-list text:nth-child(2){
  vertical-align: middle;
}
.recommend{
  padding:2rpx 3rpx;
  border:1rpx solid #F8CE3A;
  border-radius:8rpx;
  margin-right:8rpx;
  font-size:23rpx;
  color:#F8CE3A;

}

.on{
  color: #E6AD14;
} 

.on .line{
    width: 20%;
    margin: 0 auto;
    border-bottom: 3rpx solid #E6AD14;
    position: absolute;
    bottom: 0rpx;
    left: 50%;
   transform: translateX(-50%);
   transform: -webkit-translateX(-50%);
}
